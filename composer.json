{
  "name": "jpry/ynab-sdk-php",
  "description": "Framework-agnostic YNAB API client for PHP",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "guzzlehttp/psr7": "^2.7",
    "psr/http-client": "^1.0",
    "psr/http-message": "^2.0"
  },
  "require-dev": {
    "pestphp/pest": "^4.0"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Use the built-in Guzzle request sender"
  },
  "autoload": {
    "psr-4": {
      "JPry\\YNAB\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JPry\\YNAB\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "pest"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
